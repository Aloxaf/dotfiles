# Load the oh-my-zsh's library.
antigen use oh-my-zsh

# Bundles from the default repo (robbyrussell's oh-my-zsh).
antigen bundle cargo
antigen bundle fd
antigen bundle git
antigen bundle pip
antigen bundle httpie
antigen bundle sudo
antigen bundle extract
antigen bundle rust
antigen bundle colored-man-pages

antigen bundle paulirish/git-open
antigen bundle zdharma/fast-syntax-highlighting
antigen bundle zsh-users/zsh-autosuggestions
antigen bundle zsh-users/zsh-completions
antigen bundle skywind3000/z.lua

# Load the custom settings
CUSTOM=$HOME/dotfiles/zsh/.oh-my-zsh/custom
antigen bundle $CUSTOM/themes loli.zsh-theme  --no-local-clone
antigen bundle $CUSTOM/plugins skim.plugin.zsh --no-local-clone
antigen bundle $CUSTOM/plugins alias.plugin.zsh --no-local-clone

# Tell Antigen that you're done.
antigen apply
